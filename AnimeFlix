var listaAnimes = [
  "data:image/jpeg;base64,",
  "data:image/jpeg;base64,",
  "data:image/jpeg;base64,"
];
var nomesAnimes = ["One Piece", "Naruto", "Demon Slayer"];

let listaImg = document.getElementById("listaImagens");

preencheImagensInicial();

function adicionarAnime() {
  var nomeAnimeFavorito = document.getElementById("nomeAnime").value;
  var animeFavorito = document.getElementById("anime").value;

  if (animeFavorito.endsWith(".jpg") && nomeAnimeFavorito != "") {
    verificaDuplicacao(animeFavorito, nomeAnimeFavorito);
  } else {
    alert("Endereço de anime inválido ou campo vazio");
  }
  document.getElementById("anime").value = "";
  document.getElementById("nomeAnime").value = "";
}
function removerAnime() {
  var nomeAnimeRemover = document.getElementById("nomeAnime").value;
  if (removerAnime == "") {
    alert("Por favor, insira o nome do anime");
  } else {
    verificaCatalogo(nomeAnimeRemover);
  }

  document.getElementById("nomeAnime").value = "";
}
function verificaCatalogo(nome) {
  var flag = false;
  for (var j = 0; j < nomesAnimes.length; j++) {
    if (nome == nomesAnimes[j]) {
      flag = true;
      listaAnimes.splice(j, 1);
      nomesAnimes.splice(j, 1);
      apagaImagem(j);
    }
  }
  if (flag == false) {
    alert("Esse anime não está no catálogo");
  }
}
function verificaDuplicacao(anime, nome) {
  var verificacao = false;
  for (var elemento = 0; elemento < listaAnimes.length; elemento++) {
    if (anime == listaAnimes[elemento]) {
      alert("Esse anime já está no catálogo");
      verificacao = true;
    }
  }
  if (verificacao == false) {
    nomesAnimes.push(nome);
    listaAnimes.push(anime);
    adicionaImagem(anime);
  }
}

function adicionaImagem(anime) {
  var indice = listaAnimes.indexOf(anime);

  let li = document.createElement("li");
  li.innerHTML =
    "<img src =" + listaAnimes[indice] + "><br>" + nomesAnimes[indice];
  listaImg.appendChild(li);
}
function apagaImagem(indice) {
  var itensLista = document.getElementsByTagName("li");
  listaImg.removeChild(itensLista[indice]);
}
function preencheImagensInicial() {
  listaAnimes.forEach((anime) => adicionaImagem(anime));
}
